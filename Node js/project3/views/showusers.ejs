<%- include('menu')%>
<div class="container">
    <h1>Show User</h1>

    <%=data%>

    <table class="table">
  <thead>
    <tr>
      <th scope="col">no</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Mobile</th>
    </tr>
  </thead>
  <tbody>
    <% data.forEach(function(value,i){%>
    <tr>
      <th scope="row"><%=i+1%></th>
      <td><%=value.name%></td>
      <td><%=value.email%></td>
      <td><%=value.mobile%></td>
    </tr>
    
    <%})%>
  </tbody>
</table>
</div>